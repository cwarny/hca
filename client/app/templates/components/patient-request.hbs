{{!-- <th>
	<span class="clickable glyphicon glyphicon-ok checkmark {{if req.completed "checked"}}" {{action "complete" req}}></span>
</th> --}}
<td>
	{{req.message}}
</td>
<td>
	{{req.from}}
</td>
<td>
	{{ago req.timestamp}}
</td>
<td>
	{{#if req.handler}}
		Handled by {{req.handler}}
		{{#if owned}}
			<button class="btn btn-sm btn-default" {{action "release" req}}>Release</button>
			<button class="btn btn-sm btn-default" {{action "complete" req}}>Complete</button>
		{{/if}}
	{{else}}
		<button class="btn btn-sm btn-default" {{action "handle" req}}>Handle</button>
	{{/if}}
</td>